
config wifidog 'wifidog'
	option gateway_interface 'br-lan'
	option gateway_eninterface 'eth0'
	option gateway_httpport '80'
	option gateway_connmax '200'
	option wifidog_enable '0'
	option enable '0'
	option gateway_id 'live-8.taobao.com'
	option gateway_eniterface 'eth1'
	option gateway_hostname '42.121.98.148'
	option gateway_path '/as/s/'
	option HTTPDMaxConn '15'

config wifidog_deamo 'wifidog_deamo'
	option enable '1'


